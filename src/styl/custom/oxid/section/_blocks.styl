	ul.block-layered-nav 					{ border:0; margin:0; padding:0; display:none; /* No product filters */ background:#3E4042 url(../images/bkg_showcase.gif) repeat-x; font-size:0.9em; }
	.block-layered-nav>li.block-subtitle	{ padding:0; border-width:1px 0; display:block; position:relative;  height:71px; line-height:71px; overflow:hidden; text-transform:uppercase;}
	.block-layered-nav>li.block-subtitle>a 	{ background:none; height:30px; line-height:30px; padding:20px 0 20px 10px; display:block; text-shadow:1px 1px #222; color:#808080; text-align:left;}
	.block-layered-nav>li.block-subtitle>a span		{ background:transparent url(../images/iconbar2.gif) no-repeat left -347px; padding:0 0 0 30px; height:30px; line-height:30px; display:block;}
	.block-layered-nav>li:hover>a,
	.block-layered-nav>li.block-subtitle>a:hover		{ color:#fff; background:#494a4a; box-shadow: 0 6px 10px #333;}
	.block-layered-nav>li dl				{ width:960px; height:auto; position:absolute; top:70px;  left:-9999em; z-index:999; background:#494a4a; padding:0; line-height:1.6em; box-shadow: 0 6px 10px #333;}
	.block-layered-nav>li:hover				{ overflow:visible; z-index:1001; }
	.block-layered-nav>li:hover dl 			{ left:0px;}
	.block-layered-nav .block-content 		{ padding:20px; text-align:center;}
	.block-layered-nav dt 					{ padding:7px 10px 0 10px; color:#000; font-weight:bold; text-transform:uppercase; }
	.block-layered-nav dd					{ margin:0;}
	.block-layered-nav span.field 			{ float:left; width:24%; margin-bottom:15px; display:inline-block; border-right: 1px solid #6e6f70; }
	.block-layered-nav dd li				{ padding: 0px 4px 0px 8px; color:#8a8b8b;}
	.block-layered-nav dd.last 				{ }
	.block-layered-nav dd li a,
	.block-layered-nav dd li a:visited		{ display:block; padding:0 10px; color:#b1b3b3; text-shadow:none;}
	.block-layered-nav dd li a:hover		{ color:#fff; background-color:#6e6f70; text-decoration:none;}	
	.block-layered-nav dd li a span.count 	{ color:#999; margin-left:5px;}
	.block-layered-nav li.currently 		{ padding:0 0 10px 0;}
	.block-layered-nav li.currently > span 	{ background:none; padding:0 0 0 20px; color:#db2525; text-transform:uppercase;}
	.block-layered-nav li.currently ol		{ padding:0 20px; }
	.block-layered-nav .currently li 		{ padding:4px 0; float:none; color:#808080; font-weight:bold;}
	.block-layered-nav .currently .label 	{ font-weight:normal; color:#737475;}
	.block-layered-nav .currently .btn-remove { margin:0; float:right; }
	.block-layered-nav .actions 			{ font-size:1em; padding:4px 20px 0 0;  border-width:1px 0; text-align:right; }
	.block-layered-nav .actions a 			{ clear:both; float:none; color:#808080; text-transform:uppercase; font-size:0.9em;}
	.block-layered-nav .actions a:hover		{ color:#fff;}


