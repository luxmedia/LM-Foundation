/* Sidebar =============================================================================== */
	.block 										{ line-height:1.3em; margin: 0 0 10px 0; background: #333; position:relative; }
	.block:first-of-type 						{ border-top:none; }
	.sidebar .block								{ margin-bottom:10px;}
	.sidebar .block:last-child 					{ margin:0; }
	.block .block-title 						{ display:block; font-size:1.3em; font-weight:bold; line-height:1.5em; padding:7px 10px; margin:0; }
	.cms-home .block-title,
	.cms-home .box .h2							{ font-size: 1.5em; }
	.block .block-title strong 					{ font-weight:bold; letter-spacing:1px; }
	.block .block-title strong span 			{ }
	.block .block-title a,
	.block .block-title a:visited				{ color:#737475; text-decoration:none !important;}
	.block .block-title a:hover					{ color:#fff;}
	.block .block-title span 					{ color: #999; padding: 0; display:block; }
	.block .block-title span .count 			{ text-transform:none; color:#666; white-space:nowrap; font-weight:normal; font-size:.95em; }
	.block h5 									{ font-size:1em; }	
	.block .block-subtitle 						{ margin:0; padding:5px 9px; font-size:1em; font-weight:bold; color:#666; }
	.block .block-content 						{ color:#999; margin:0; padding:20px; }
	.block .block-content li					{ position:relative; background:none; }
	.block .block-content li.item 				{ border-bottom: none; padding:0; margin:0; }
	.block .block-content li label				{ color:#666;}
	.block-content a,
	.block-content a:visited					{ color:#ddd; text-decoration:none; }
	.block-content a:hover						{ color:#fff; text-decoration:underline;}
	.block .block-content li a					{ border-bottom: 1px solid #444; color:#ccc; display:block; margin:0; padding:5px 20px 5px 0; background:transparent url(../../../images/arrow_right.gif) no-repeat right 5px;}
	.block .block-content li a span.date		{ color:#999; display:inline; float:right;}
	.block .block-content li a:hover			{ color:#db2525; text-decoration:none; border-color:#db2525; }
	.block .block-content li a:hover span.date	{ color:#fff;}
	.block .btn-remove 							{ float:right; margin:1px 0 2px 5px; }
	.block .actions 							{ padding:6px 0; text-align:right; }
	.block .actions a,
	.block .actions .button 					{ /*float:right;*/ float:none; display:block; margin:5px 0; }
	.block .empty 								{ margin:0; padding:5px 9px; }
	
	.block li.odd 								{ background:transparent; }
	.block li.even 								{ }
	
	.block span.block-links						{ position:absolute; right:20px; top:20px;}
	.col-left .block span.block-links,
	.col-right .block span.block-links			{ top:10px; right:10px; }
	.block a.link-more,
	.block a.link-more:visited					{ float:right; color:#808080; text-transform:uppercase; text-align:right; font-size:1.1em; }
	.block a.link-more:hover					{ color:#fff; }
	.block a.link-more span						{ position:relative; top:-1px; font-weight:bold;} /* &rang icon beside text */
	
	.block .block-content .product-name a		{ color:#fff; }
	.block .block-content .product-name a:hover { color:#db2525; }	
	
	/* Prices */
	.block .regular-price,
	.block .regular-price .price 				{ color:#ccc; font-size:1.1em; }
	.block .tax-details 						{ display:none; text-align:right; font-size:0.9em; }	
	
	/* Blank */
	.block_blank 								{ background:none; padding:0; }
	.block_blank .block-content 				{ padding:0; }
	.block_blank .block-title 					{ display:none; }
	
